{
    "expo": {
        "name": "Nubank-Clone",
        "slug": "nubank-app",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "nubankapp",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/android-icon-foreground.png",
                "backgroundColor": "#8308d1"
            },
            "edgeToEdgeEnabled": true,
            "predictiveBackGestureEnabled": false,
            "package": "com.anonymous.nubankapp"
        },
        "web": {
            "output": "static",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": [
            "expo-router",
            [
                "expo-splash-screen",
                {
                    "image": "./assets/images/splash-icon.png",
                    "imageWidth": 160,
                    "resizeMode": "contain",
                    "backgroundColor": "#8308d1",
                    "dark": {
                        "backgroundColor": "#8308d1"
                    }
                }
            ],
            "expo-web-browser",
            "expo-secure-store"
        ],
        "experiments": {
            "typedRoutes": true,
            "reactCompiler": true
        },
        "extra": {
            "router": {},
            "eas": {
                "projectId": "8e98de55-d876-4295-bd0f-d3e2c69ae71f"
            }
        },
        "runtimeVersion": {
            "policy": "appVersion"
        },
        "updates": {
            "url": "https://u.expo.dev/8e98de55-d876-4295-bd0f-d3e2c69ae71f"
        }
    }
}
